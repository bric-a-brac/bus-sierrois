
<ul class="stops">
{% for stop in stops %}
	<li id="stop-{{ stop.id }}" class="stop">
	{{ stop.id }}<br/>
	{{ stop.name }}<br/>
	{{ stop.coordinate.latitude }}, {{ stop.coordinate.longitude }}<br/>
	<span id="stop-{{ stop.id }}-distance">aa {{ stop.distance }}</span>
	</li>
{% endfor %}
</ul>

